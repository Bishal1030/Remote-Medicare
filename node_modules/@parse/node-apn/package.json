{
  "name": "@parse/node-apn",
  "description": "An interface to the Apple Push Notification service for Node.js",
  "version": "5.2.3",
  "author": "Parse Platform, Andrew Naylor <argon@mkbot.net>",
  "keywords": [
    "apple",
    "push",
    "push notifications",
    "iOS",
    "apns",
    "notifications"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/parse-community/node-apn.git"
  },
  "dependencies": {
    "debug": "4.3.3",
    "jsonwebtoken": "9.0.0",
    "node-forge": "1.3.1",
    "verror": "1.10.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/node": "^14.14.40",
    "chai": "^4.3.4",
    "chai-as-promised": "7.1.1",
    "codecov": "3.8.1",
    "eslint": "^8.2.0",
    "mocha": "^8.3.2",
    "nyc": "15.1.0",
    "prettier": "^2.4.1",
    "semantic-release": "^17.4.6",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "coverage": "node_modules/.bin/nyc --reporter=lcov node_modules/.bin/mocha 'test/**/*.js'",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier",
    "eslint": "eslint --cache ./",
    "eslint:fix": "eslint --cache --fix ./",
    "prettier": "prettier --write {lib,test,examples,mock}/{**/*,*}.js index.js",
    "test": "node_modules/.bin/mocha 'test/**/*.js'"
  },
  "jshintConfig": {
    "node": true
  },
  "eslintConfig": {
    "ecmaVersion": 6,
    "env": {
      "es6": true,
      "node": true
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "license": "MIT"
}
